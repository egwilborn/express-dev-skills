<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>Express Developer Skills</title>
  </head>
  <body>
    <a href="/">Home</a>
    <h1>Express Developer Skills</h1>
    <ul>
      <% skills.forEach(function(skill) { %>
      <li>
        <a href="/skills/<%=skill.id%>"><%= skill.todo %></a>
        - <%= skill.done ? 'done' : 'not done' %>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
